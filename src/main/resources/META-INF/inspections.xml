<idea-plugin>
    <extensions defaultExtensionNs="com.intellij">
        <localInspection language="RuneScript"
                         groupName="RuneScript"
                         displayName="Unresolved procedure"
                         enabledByDefault="true"
                         level="ERROR"
                         implementationClass="io.runescript.plugin.ide.inspections.RuneScriptUnresolvedProcedureInspection"/>
        <localInspection language="RuneScript"
                         groupName="RuneScript"
                         displayName="Unresolved clientscript"
                         enabledByDefault="true"
                         level="ERROR"
                         implementationClass="io.runescript.plugin.ide.inspections.RuneScriptUnresolvedClientscriptInspection"/>
        <localInspection language="RuneScript"
                         groupName="RuneScript"
                         displayName="Unresolved reference"
                         enabledByDefault="true"
                         level="ERROR"
                         implementationClass="io.runescript.plugin.ide.inspections.RuneScriptUnresolvedReferenceInspection"/>
        <localInspection language="RuneScript"
                         groupName="RuneScript"
                         displayName="Unresolved local variable"
                         enabledByDefault="true"
                         level="ERROR"
                         implementationClass="io.runescript.plugin.ide.inspections.RuneScriptUnresolvedLocalVariableInspection"/>
        <localInspection language="RuneScript"
                         groupName="RuneScript"
                         displayName="Duplicate local variable"
                         enabledByDefault="true"
                         level="ERROR"
                         implementationClass="io.runescript.plugin.ide.inspections.RuneScriptDuplicateLocalVariableInspection"/>
        <localInspection language="RuneScript"
                         groupName="RuneScript"
                         displayName="Unresolved game variable"
                         enabledByDefault="true"
                         level="ERROR"
                         implementationClass="io.runescript.plugin.ide.inspections.RuneScriptUnresolvedScopedVariableInspection"/>
        <localInspection language="RuneScript"
                         groupName="RuneScript"
                         displayName="Unresolved constant"
                         enabledByDefault="true"
                         level="ERROR"
                         implementationClass="io.runescript.plugin.ide.inspections.RuneScriptUnresolvedConstantInspection"/>
        <localInspection language="RuneScript"
                         groupName="RuneScript"
                         displayName="Unresolved command"
                         enabledByDefault="true"
                         level="ERROR"
                         implementationClass="io.runescript.plugin.ide.inspections.RuneScriptUnresolvedCommandInspection"/>
        <localInspection language="RuneScript"
                         groupName="RuneScript"
                         displayName="Type checking"
                         enabledByDefault="true"
                         level="ERROR"
                         implementationClass="io.runescript.plugin.ide.inspections.RuneScriptTypeCheckerInspection"/>
        <localInspection language="RuneScript"
                         groupName="RuneScript"
                         displayName="Unreachable code"
                         enabledByDefault="true"
                         level="WARNING"
                         implementationClass="io.runescript.plugin.ide.inspections.RuneScriptUnreachableCodeInspection"/>
        <localInspection language="RuneScript"
                         groupName="RuneScript"
                         displayName="Unused local variable"
                         enabledByDefault="true"
                         level="WARNING"
                         implementationClass="io.runescript.plugin.ide.inspections.RuneScriptUnusedLocalVariableInspection"/>
        <localInspection language="RuneScript"
                         groupName="RuneScript"
                         displayName="Missing script symbol"
                         enabledByDefault="true"
                         level="ERROR"
                         implementationClass="io.runescript.plugin.ide.inspections.RuneScriptMissingScriptSymbolInspection"/>
        <localInspection language="RuneScriptSymbol"
                         groupName="RuneScript"
                         displayName="Invalid symbol file"
                         enabledByDefault="true"
                         level="ERROR"
                         implementationClass="io.runescript.plugin.ide.inspections.RuneScriptInvalidSymbolFileInspection"/>
    </extensions>
</idea-plugin>